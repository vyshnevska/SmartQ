- if user_signed_in?
  %li= link_to I18n.t('views.nav.profile'), user_path(current_user)
  %li= link_to I18n.t('views.nav.logout'), destroy_user_session_path, :method=>'delete'
  %li= link_to I18n.t('views.nav.categories'), admin_categories_path
  %li= link_to current_user.role.capitalize, '#'
  - if current_user.admin?
    %li= link_to I18n.t('views.nav.users'), users_path
  - else
    %li= link_to I18n.t('views.nav.assessment'), user_assessments_path
- else
  %li= link_to I18n.t('views.nav.sign_in'), new_user_session_path
  %li= link_to I18n.t('views.nav.sign_up'), new_user_registration_path